{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/static/sitemap.xml",
      "headers": {
        "Content-Type": "application/xml; charset=utf-8",
        "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0"
      }
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
